input RepaymentInput {
    name: String!
    description: String!
    amount: Decimal!
    type: String!
    currencyId: ID!
    expenseAt: Time!
    debtor: ID!
    creditor: ID!
}

input ExpenseInput {
    name: String!
    description: String!
    amount: Decimal!
    type: String!
    currencyId: ID!
    expenseAt: Time!
    payers: [ShareInput!]!
    shares: [ShareInput!]!
}

input ShareInput {
    userId: ID!
    amount: Decimal!
}